{
  "schemaVersion": 1,
  "id": "opt-carpet-addition",
  "version": "${version}",
  "name": "Opt-Carpet-Addition",
  "description": "An extension mod of fabric-carpet.",
  "authors": [
    "OptiJava"
  ],
  "contact": {
    "homepage": "https://github.com/OptiJava/OptCarpetAddition",
    "sources": "https://github.com/OptiJava/OptCarpetAddition"
  },
  "license": "CC0-1.0",
  "icon": "assets/opt-carpet-addition/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.optijava.opt_carpet_addition.OptCarpetAddition"
    ]
  },
  "mixins": [
    "opt-carpet-addition.mixins.json"
  ],
  "accessWidener": "opt-carpet-addition.accesswidener",
  "depends": {
    "fabricloader": ">=0.7.4",
    "minecraft": "${minecraft_requirement_version}",
    "fabric": ">=0.39.2",
    "carpet": "${carpet_requirement_version}"
  },
  "suggests": {
    "another-mod": "Carpet-TIS-Addition"
  }
}
